    "ava": "^0.15.2",
    "babel-eslint": "^6.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.1",
    "debug": "^2.2.0",
    "eslint": "^3.5.0",
    "node-sass": "^3.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "ava": {
